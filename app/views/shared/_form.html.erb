<%= form_for ([object]) do |f| %>

	<div class = "container">	
		<p>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</p>

		<p>
			<%= f.label :duration %>
			<%= f.text_field :duration %>
		</p>

		<p>
			<%= f.label :hours %>
			<%= f.text_field :hours %>
		</p>

		<p>
			<%= f.label :location %>
			<%= f.text_field :location %>
		</p>

		<%= f.submit %>
	</div>


<% end %>